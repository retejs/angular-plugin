{
  "name": "rete-angular-plugin",
  "version": "2.1.2",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:legacy && npm run build:12 && npm run build:13 && npm run build:14 && npm run build:15 && npm run build:16 && npm run build:17 && npm run build:18",
    "build:legacy": "ng-packagr -p ng-package.js",
    "build:12": "npm --prefix ng12 run build",
    "build:13": "npm --prefix ng13 run build",
    "build:14": "npm --prefix ng14 run build",
    "build:15": "npm --prefix ng15 run build",
    "build:16": "npm --prefix ng16 run build",
    "build:17": "npm --prefix ng17 run build",
    "build:18": "npm --prefix ng18 run build",
    "postinstall": "npm i --prefix ng12 && npm i --prefix ng13 && npm i --prefix ng14 && npm i --prefix ng15 && npm i --prefix ng16 && npm i --prefix ng17 && npm i --prefix ng18",
    "doc": "rete doc"
  },
  "author": "Vitaliy Stoliarov",
  "license": "MIT",
  "keywords": [
    "angular",
    "plugin",
    "rete",
    "Rete.js"
  ],
  "homepage": "https://retejs.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retejs/angular-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/retejs/angular-plugin/issues"
  },
  "peerDependencies": {
    "@angular/common": ">= 12 < 18",
    "@angular/core": ">= 12 < 18",
    "@angular/elements": ">= 12 < 18",
    "rete": "^2.0.1",
    "rete-area-plugin": "^2.0.0",
    "rete-render-utils": "^2.0.0",
    "rxjs": ">=6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4 || ~0.12.0 || ~0.13.0 || ~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "^12.2.17",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/elements": "~12.2.0",
    "@types/estree": "^1.0.0",
    "@types/node": "^12.11.1",
    "ng-packagr": "^12.1.1",
    "rete": "^2.0.1",
    "rete-area-plugin": "^2.0.0",
    "rete-cli": "^1.0.2",
    "rete-render-utils": "^2.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "typescript": "~4.3.5",
    "zone.js": "~0.11.4"
  }
}
